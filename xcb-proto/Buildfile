PACKAGE="xcb-proto"
VER="1.14.1"

ARCH="installer"
ACCESS="restricted"
CRYPT="key"

DESCRIPTION="XCB Protocol descriptions"

DEPENDS=()
CONFLICTS=()

URL="https://xorg.freedesktop.org/archive/individual/proto/"
SRCDIR="${PACKAGE}-${VER}"
SRCTGZ="${SRCDIR}.tar.xz"
SRCURL="${URL}${SRCTGZ}"

__getsrc(){
    wget -q $SRCURL
}
__unpacksrc(){
    tar xJf $SRCTGZ
}
__preparesrc(){
    ./configure $CONFIGUREOPTS
}
__compilesrc(){
    make $MAKEOPTS
}
__installsrc(){
    make $MAKEOPTS DESTDIR="$DEST" install
}
